{
  "plugins": ["vue"],

  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },

  "parser": "babel-eslint",

  "extends": ["eslint:recommended"],

  "rules": {
    "indent": ["error", 2],
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "semi": ["error", "never"],
    "no-console": 0
  }
}
